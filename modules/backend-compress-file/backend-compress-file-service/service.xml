<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="vn.gt.dao">
	<author>huymq</author>
	<namespace>compressfile</namespace>
	<entity name="CompressFileJob" local-service="true" remote-service="false">

		<!-- PK fields -->

		<column name="id" type="long" primary="true" />

		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />
		<column name="listHoSoId" type="String" />
		<column name="lastedHoSoId" type="long" />
		<column name="status" type="boolean" />

		<!-- Order -->

		<order by="desc">
			<order-column name="createDate" />
		</order>
	</entity>

	<entity name="CompressFileLog" local-service="true" remote-service="false">

		<!-- PK fields -->

		<column name="id" type="long" primary="true" />

		<column name="createDate" type="Date" />
		<column name="hoSoId" type="long" />
		<column name="fileEntryId" type="long" />
		<column name="oldSize" type="long" />
		<column name="newSize" type="long" />
		<column name="status" type="boolean" />
		<column name="errorMsg" type="String" />

		<!-- Order -->

		<order by="desc">
			<order-column name="createDate" />
		</order>

		<!-- Finder methods -->

		<finder name="HoSoId" return-type="Collection">
			<finder-column name="hoSoId" />
		</finder>
		
		<finder name="FileEntryId" return-type="CompressFileLog">
			<finder-column name="fileEntryId" />
		</finder>
	</entity>
</service-builder>